<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- tabhost文字颜色 -->
    <!-- Non focused states -->
    <item android:state_focused="false" android:state_pressed="false" android:state_selected="false" android:drawable="@color/base_menu_bg"/>
    <item android:state_focused="false" android:state_pressed="false" android:state_selected="true" android:drawable="@color/base_menu_bg"/>
    <!-- Focused states -->
    <item android:state_focused="true" android:state_pressed="false" android:state_selected="false" android:drawable="@color/translate"/>
    <item android:state_focused="true" android:state_pressed="false" android:state_selected="true" android:drawable="@color/translate"/>
    <!-- Pressed -->
    <item android:state_pressed="true" android:state_selected="true" android:drawable="@color/main_out_down_line_color"/>
    <item android:state_pressed="true" android:drawable="@color/main_out_down_line_color"/>

</selector>