    <!-- 主页面中间说明部分 -->
    <style name="textStyle">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/main_text_color</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="wrap">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="image_arrow">
        <item name="android:layout_width">13dp</item>
        <item name="android:layout_height">20dp</item>
    </style>

    <!-- 安全中心 style -->
    <style name="sec" parent="wrap">
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/blue_text_base</item>
    </style>

    <!-- 安全中心 style -->
    <style name="sec_00" parent="wrap">
        <item name="android:layout_marginTop">20dp</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/blue_text_base</item>
    </style>

    <!-- 安全中心 style -->
    <style name="sec_01" parent="wrap">
        <item name="android:layout_marginTop">20dp</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/assets_text_a</item>
    </style>
    <!-- 安全中心 style -->
    <style name="sec_02" parent="wrap">
        <item name="android:layout_marginTop">25dp</item>
        <item name="android:layout_marginRight">8dp</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/gray_1</item>
    </style>
    <!-- 主页面中间部分间隔 -->
    <style name="separate_wrap">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">58dp</item>
    </style>

    <style name="edit_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">13dp</item>
        <item name="android:textSize">15dp</item>
        <item name="android:singleLine">true</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:padding">11dp</item>
        <item name="android:background">@null</item>
        <item name="android:textColorHint">@color/gray_hint</item>
        <item name="android:textColor">@color/black</item>
    </style>

    // 这个风格是专门给转让使用的,和上面的不一样，上面的是共用的 @author Ysw created at 2017/2/21 11:23
    <style name="transfer_edit_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">0dp</item>
        <item name="android:textSize">15dp</item>
        <item name="android:singleLine">true</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:padding">11dp</item>
        <item name="android:background">@null</item>
        <item name="android:textColorHint">@color/gray_hint</item>
        <item name="android:textColor">@color/black</item>
    </style>


    <style name="edit_style_new">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">13dp</item>
        <item name="android:textSize">14sp</item>
        <item name="android:singleLine">true</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:background">@null</item>
        <item name="android:textColorHint">@color/gray_hint</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <!-- 图形解锁的样式 -->

    <style name="ButtonGreen" parent="@android:style/Widget.Button">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textColor">@drawable/skin_color_button</item>
        <item name="android:background">@drawable/common_btn_green</item>
    </style>

    <style name="ButtonRed" parent="@android:style/Widget.Button">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textColor">@drawable/skin_color_button</item>
        <item name="android:background">@drawable/common_btn_red</item>
    </style>

    <style name="rbtn_bank" parent="@android:style/Widget.CompoundButton.RadioButton">
        <item name="android:button">@null</item>
        <item name="android:height">50dp</item>
        <item name="android:textSize">17sp</item>
        <item name="android:drawableRight">@drawable/rbtn_bank_check</item>
        <item name="android:background">@drawable/rbtn_bank_bg</item>
        <item name="android:paddingLeft">20dp</item>
        <item name="android:paddingRight">20dp</item>
    </style>

    <!-- 自定义Dialog的样式 -->
    <style name="MyDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:layout_marginRight">0dp</item>
        <item name="android:layout_marginLeft">0dp</item>
    </style>

    <!-- 收不到短信Dialog的样式 -->
    <style name="NoMassageDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:layout_marginRight">15dp</item>
        <item name="android:layout_marginLeft">15dp</item>
    </style>

    <style name="MyDialogWide" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
    </style>
    <!-- 字体样式 -->
    <style name="text_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">18sp</item>
    </style>

    <!-- 实线 -->
    <style name="line">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1px</item>
        <item name="android:background">@color/one_dp_line</item>
    </style>

    <style name="lineshape">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">3dp</item>
        <item name="android:background">@drawable/line_shape</item>
    </style>

    <style name="line_dialog">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">@color/new_line_color</item>
    </style>

    <style name="line_dialog_market">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">@color/weixin_pre_bg</item>
    </style>
    <!-- 字体样式 -->
    <style name="txt_about_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">@color/bule_text_bg</item>
        <item name="android:padding">16dp</item>
        <item name="android:textSize">19sp</item>
    </style>

    <!-- 安全中心,银行图标大小 -->
    <style name="sec_image_bank_icon">
        <item name="android:layout_width">50dp</item>
        <item name="android:layout_height">50dp</item>
    </style>

    <!-- 安全中心,银行名称字体 -->
    <style name="sec_bank_name" parent="wrap">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/black</item>
    </style>
    <!-- 安全中心,银行尾号字体 -->
    <style name="sec_bank_no" parent="wrap">
        <item name="android:layout_marginTop">30dp</item>
        <item name="android:layout_marginLeft">70dp</item>
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">@color/gray</item>
    </style>

    <!-- 短信验证码输入框 -->
    <style name="captcha_style">
        <item name="android:layout_height">match_parent</item>
        <item name="android:textSize">15sp</item>
        <item name="android:singleLine">true</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:background">@null</item>
        <item name="android:textColorHint">@color/gray_hint</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="btn_code">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
    </style>
    <!-- 密码盘dialog样式 -->
    <style name="PasswordDialog" parent="android:style/Theme.Dialog">
        <item name="android:windowNoTitle">true</item>
        <item name="android:background">#00000000</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:backgroundDimAmount">0.7</item>
    </style>
    <!-- 分享对话框样式 -->
    <style name="ShareDialog" parent="@android:Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>

    <style name="Dialog_Fullscreen" parent="@android:style/Theme.Dialog">
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>
</resources>