<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/base_style_bg"
    android:orientation="vertical" >

    <!-- title布局 -->

    <include
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        layout="@layout/title" />

    <View style="@style/line" />

    <View
        style="@style/line"
        android:layout_marginTop="20dp" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/all_menu_height"
                android:background="@color/base_menu_bg" >

                <TextView
                    style="@style/sec"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/all_left_offset"
                    android:text="@string/name"
                    android:textColor="@color/main_out_down_text_color" />

                <TextView
                    android:id="@+id/bank_detail_tv1"
                    style="@style/sec"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/all_left_offset"
                    android:text=""
                    android:textColor="@color/main_out_down_text_color" />
            </RelativeLayout>

            <View style="@style/line" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/all_menu_height"
                android:background="@color/base_menu_bg" >

                <TextView
                    style="@style/sec"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/all_left_offset"
                    android:text="@string/select_bank_card"
                    android:textColor="@color/main_out_down_text_color" />

                <TextView
                    android:id="@+id/bank_detail_tv2"
                    style="@style/sec"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/all_left_offset"
                    android:text=""
                    android:textColor="@color/main_out_down_text_color" />
            </RelativeLayout>

            <View style="@style/line" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/all_menu_height"
                android:background="@color/base_menu_bg" >

                <TextView
                    style="@style/sec"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/all_left_offset"
                    android:text="@string/bank_card_no"
                    android:textColor="@color/main_out_down_text_color" />

                <TextView
                    android:id="@+id/bank_detail_tv3"
                    style="@style/sec"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/all_left_offset"
                    android:text=""
                    android:textColor="@color/main_out_down_text_color" />
            </RelativeLayout>

            <View style="@style/line" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/all_menu_height"
                android:background="@color/base_menu_bg" >

                <TextView
                    style="@style/sec"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/all_left_offset"
                    android:text="@string/bank_card_city"
                    android:textColor="@color/main_out_down_text_color" />

                <TextView
                    android:id="@+id/bank_detail_tv4"
                    style="@style/sec"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/all_left_offset"
                    android:text=""
                    android:textColor="@color/main_out_down_text_color" />
            </RelativeLayout>

            <View style="@style/line" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/all_menu_height"
                android:background="@color/base_menu_bg" >

                <TextView
                    style="@style/sec"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/all_left_offset"
                    android:text="@string/bank_card_branch"
                    android:textColor="@color/main_out_down_text_color" />

                <TextView
                    android:id="@+id/bank_detail_tv5"
                    style="@style/sec"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/all_left_offset"
                    android:text=""
                    android:textColor="@color/main_out_down_text_color" />
                <!-- 线条 -->

            </RelativeLayout>

            <View style="@style/line" />

            <Button
                android:id="@+id/bank_detail_submit"
                style="@style/btn5_style"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/btn_bg_select"
                android:text="@string/remove_bind"
                android:textColor="@color/white" />
        </LinearLayout>
    </ScrollView>

</LinearLayout>