<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="TabLayoutStyle">
        <item name="android:textSize">16sp</item>
    </style>

    <style name="edittext5_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textSize">15dp</item>
        <item name="android:singleLine">true</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:paddingBottom">8dp</item>
        <item name="android:background">@null</item>
        <item name="android:textColorHint">@color/gray_bg_btn</item>
        <item name="android:textColor">@color/black</item>
    </style>
    <!-- text样式 -->
    <!-- 字体样式 -->
    <style name="text5_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">@color/phone_font_color</item>
        <item name="android:textSize">16sp</item>
    </style>
    <!-- layout样式 -->
    <style name="layout5_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginTop">20dp</item>
        <item name="android:padding">15dp</item>
        <item name="android:background">@drawable/layout_bg_select</item>
    </style>
    <!-- 实线 -->
    <style name="line5_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0.8dip</item>
        <item name="android:background">#e5e5e5</item>
    </style>

    <style name="line_news">
        <item name="android:layout_width">0.5dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/main_blue_color</item>
    </style>
    <!-- 设置箭头样式 -->
    <style name="jiantou5_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginRight">5dp</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:src">@drawable/button_arrow</item>
    </style>

    <!-- 设置按钮样式 -->
    <style name="btn5_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:layout_marginTop">@dimen/forget_password_height</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">18sp</item>
        <item name="android:background">@drawable/btn_bg</item>
        <item name="android:textColor">@color/gray_hint</item>
        <item name="android:padding">10dp</item>
    </style>

    <style name="btn_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">18sp</item>
        <item name="android:background">@drawable/btn_bg</item>
        <item name="android:textColor">@color/gray_hint</item>
    </style>

    <style name="btn5_style_new">
        <item name="android:layout_marginTop">@dimen/trans_left_offset</item>
        <item name="android:layout_marginBottom">@dimen/margin_left_left_menu</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:padding">10dp</item>
    </style>

    <style name="edittext5_pwd_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1.0</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">14sp</item>
        <item name="android:singleLine">true</item>
        <item name="android:padding">10dp</item>
        <item name="android:inputType">numberPassword</item>
        <item name="android:autoText">true</item>
        <item name="android:background">@color/white</item>
        <item name="android:textColorHint">@color/gray_hint</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:maxLength">1</item>
        <item name="android:cursorVisible">false</item>
    </style>

    <style name="btn_immediate_investment">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
