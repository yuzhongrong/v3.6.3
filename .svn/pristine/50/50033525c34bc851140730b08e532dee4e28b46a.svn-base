<?xml version="1.0" encoding="utf-8"?>
<!-- 收益率ListView布局item -->
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical" >

    <RelativeLayout
        android:id="@+id/relativeLayout"
        android:layout_width="match_parent"
        android:layout_height="36dp"
        android:layout_marginBottom="5dp"
        android:layout_marginLeft="13dp"
        android:layout_marginRight="13dp"
        android:layout_marginTop="6dp"
        android:background="@color/white" >

        <TextView
            android:id="@+id/pb_jdt"
            android:layout_width="320dp"
            android:layout_height="@dimen/profit_item_padding"
            android:background="@color/blue_btn_bg" />

        <TextView
            android:id="@+id/tv_y_date"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignLeft="@+id/pb_jdt"
            android:gravity="center"
            android:padding="5dp"
            android:text=""
            android:textColor="@color/white"
            android:textSize="@dimen/profit_item_tv_size" />

        <TextView
            android:id="@+id/tv_y_amount"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignRight="@id/pb_jdt"
            android:gravity="center"
            android:padding="5dp"
            android:text=""
            android:textColor="@color/white"
            android:textSize="16sp" />
    </RelativeLayout>

</LinearLayout>