<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/new_profit_bg"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginRight="10dp"
        android:gravity="left|top"
        android:orientation="horizontal" >

        <LinearLayout
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="15dp"
            android:layout_weight="1" >

            <TextView
                android:id="@+id/tv_describle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/yesterday_earn"
                android:textColor="@color/white"
                android:textSize="13sp" />

            <TextView
                android:id="@+id/rate_coupon"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/fp_rate_coupon"
                android:textColor="@color/event_dialog_text"
                android:textSize="12sp"
                android:visibility="invisible" />
        </LinearLayout>
    </LinearLayout>

    <TextView
        android:id="@+id/total_money"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/white"
        android:textSize="@dimen/font_common_10"
        android:text="@string/zero_zero"
        android:layout_centerInParent="true"/>

</RelativeLayout>