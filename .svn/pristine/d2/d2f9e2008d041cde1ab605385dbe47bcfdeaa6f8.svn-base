<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    android:id="@+id/activity_add_bank"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F9F9F9">

    <include
        android:id="@+id/title"
        layout="@layout/title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <View
        android:id="@+id/view"
        style="@style/line"
        android:layout_below="@+id/title"/>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/view">

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:animateLayoutChanges="true"
                android:background="#FFFFFF"
                android:baselineAligned="false"
                android:focusable="true"
                android:focusableInTouchMode="true"
                android:orientation="vertical">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:background="#F9F9F9">

                    <View
                        style="@style/line"
                        android:layout_alignParentBottom="true"/>

                    <TextView
                        android:id="@+id/textView2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentTop="true"
                        android:layout_marginLeft="20dp"
                        android:gravity="center_vertical"
                        android:text="银行信息加密处理，保障用卡安全"
                        android:textColor="#bbbbbb"
                        android:textSize="12dp"/>
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rl_userName"
                    android:layout_width="match_parent"
                    android:layout_height="45dp">

                    <View
                        style="@style/line"
                        android:layout_alignParentBottom="true"/>

                    <TextView
                        android:id="@+id/tv_userName"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp"
                        android:layout_toRightOf="@+id/textView16"
                        android:textColor="#333333"
                        android:textSize="16dp"/>

                    <TextView
                        android:id="@+id/textView16"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignBaseline="@+id/tv_userName"
                        android:layout_alignBottom="@+id/tv_userName"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_marginLeft="20dp"
                        android:layout_marginStart="20dp"
                        android:text="姓名\u3000"
                        android:textColor="#333333"
                        android:textSize="16dp"/>
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rl_userId"
                    android:layout_width="match_parent"
                    android:layout_height="45dp">

                    <View
                        style="@style/line"
                        android:layout_alignParentBottom="true"/>

                    <TextView
                        android:id="@+id/tv_userId"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp"
                        android:layout_toRightOf="@+id/tv_personalNumber"
                        android:textColor="#333333"
                        android:textSize="16dp"/>

                    <TextView
                        android:id="@+id/tv_personalNumber"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="20dp"
                        android:layout_marginStart="20dp"
                        android:text="身份证"
                        android:textColor="#333333"
                        android:textSize="16dp"/>
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="45dp">

                    <View
                        style="@style/line"
                        android:layout_alignParentBottom="true"/>

                    <TextView
                        android:id="@+id/textView5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="20dp"
                        android:text="银行卡"
                        android:textColor="#333333"
                        android:textSize="16dp"/>

                    <EditText
                        android:id="@+id/et_cardNum"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp"
                        android:layout_toRightOf="@+id/textView5"
                        android:layout_toLeftOf="@+id/image_delete"
                        android:background="@null"
                        android:ems="10"
                        android:hint="请输入您的银行卡号"
                        android:inputType="number"
                        android:maxLength="23"
                        android:textColor="#333333"
                        android:textColorHint="#cccccc"
                        android:textSize="16dp"/>

                    <ImageView
                        android:id="@+id/image_delete"
                        android:layout_width="30dp"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="10dp"
                        android:visibility="invisible"
                        android:src="@drawable/icon_search_delete"/>
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rl_cardType"
                    android:layout_width="match_parent"
                    android:layout_height="45dp">

                    <View
                        style="@style/line"
                        android:layout_alignParentBottom="true"/>

                    <TextView
                        android:id="@+id/tv_type"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="20dp"
                        android:text="卡类型\u3000"
                        android:textColor="#333333"
                        android:textSize="16dp"/>

                    <ImageView
                        android:id="@+id/image_cara"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="20dp"
                        android:src="@drawable/image_card"
                        android:visibility="gone"/>

                    <ImageView
                        android:id="@+id/image_more"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="15dp"
                        android:src="@drawable/button_arrow"/>

                    <TextView
                        android:id="@+id/tv_cardType"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_toLeftOf="@+id/image_more"
                        android:layout_toRightOf="@+id/tv_type"
                        android:text="请选择银行卡类型"
                        android:textColor="#bbbbbb"
                        android:textSize="16dp"/>
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rl_phoneNum"
                    android:layout_width="match_parent"
                    android:layout_height="45dp">

                    <TextView
                        android:id="@+id/tv_phoneNum"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="20dp"
                        android:text="手机号码"
                        android:textColor="#333333"
                        android:textSize="16dp"/>

                    <ImageView
                        android:id="@+id/image_notice"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="15dp"
                        android:src="@drawable/notice"/>

                    <EditText
                        android:id="@+id/et_phoneNum"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:layout_toLeftOf="@+id/image_notice"
                        android:layout_toRightOf="@+id/tv_phoneNum"
                        android:background="@null"
                        android:ems="10"
                        android:hint="请输入银行预留手机号码"
                        android:inputType="number"
                        android:maxLength="11"
                        android:textColor="#333333"
                        android:textColorHint="#bbbbbb"
                        android:textSize="16dp"/>
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rl_protocol"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#F9F9F9">

                    <RelativeLayout
                        android:id="@+id/rl_choose"
                        android:layout_width="40dp"
                        android:layout_height="45dp">

                        <ImageView
                            android:id="@+id/image_choose"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:src="@drawable/chosen"/>
                    </RelativeLayout>

                    <TextView
                        android:id="@+id/textView17"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="5dp"
                        android:layout_toRightOf="@+id/rl_choose"
                        android:text="我同意"/>

                    <TextView
                        android:id="@+id/tv_deal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_toRightOf="@+id/textView17"
                        android:text="《鲸鱼用户服务协议》"
                        android:textColor="#1177E8"/>
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rl_next"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp"
                    android:layout_marginTop="70dp"
                    android:background="@drawable/gray_one">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_centerVertical="true"
                        android:text="下一步"
                        android:textColor="#FFFFFF"
                        android:textSize="16dp"/>
                </RelativeLayout>
            </LinearLayout>
        </ScrollView>
    </RelativeLayout>

</RelativeLayout>